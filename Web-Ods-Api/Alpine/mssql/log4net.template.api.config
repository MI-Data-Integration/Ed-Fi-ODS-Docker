<?xml version="1.0" encoding="utf-8" ?>
<log4net debug="$LOG4NET_INTERNAL_DEBUG">
  <appender name="ConsoleAppender" type="log4net.Appender.ConsoleAppender">
    <layout type="log4net.Layout.PatternLayout">
      <conversionPattern value="%date [%thread] %-5level %logger{1} - %message%newline" />
    </layout>
  </appender>
  <appender name="AdoNetUsageAppender" type="Michigan.Ods.log4netAppender.AsyncAdoNetAppender, Michigan.Ods" >
    <bufferSize value="$LOG4NET_USAGE_BUFFERSIZE" />
    <connectionType value="Microsoft.Data.SqlClient.SqlConnection, Microsoft.Data.SqlClient, Version=1.0.0.0,Culture=neutral,PublicKeyToken=23ec7fc2d6eaa4a5"/>
    <connectionStringName value="EdFi_Logging" />
    <connectionStringFile value="appsettings.json" />
    <ReconnectOnError value="true" />
    <commandText value="insert into &quot;api&quot;.&quot;usage&quot; (&quot;date&quot;,&quot;thread&quot;,&quot;level&quot;,&quot;server&quot;,&quot;logger&quot;,&quot;method&quot;,&quot;url&quot;,&quot;key&quot;,&quot;year&quot;,&quot;responsecode&quot;,&quot;starttime&quot;,&quot;endtime&quot;,&quot;dataexchangemessage&quot;,&quot;correlationId&quot;) values (@log_date,@thread,@log_level,@server,@logger,@method,@url,@key,@year,@responseCode,@startTime,@endTime,@dataExchangeMessage,@correlationId);" />
    <parameter>
      <parameterName value="@log_date" />
      <dbType value="DateTime" />
      <layout type="log4net.Layout.RawTimeStampLayout" />
    </parameter>
    <parameter>
      <parameterName value="@thread" />
      <dbType value="String" />
      <size value="255" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%thread" />
      </layout>
    </parameter>
    <parameter>
      <parameterName value="@log_level" />
      <dbType value="String" />
      <size value="50" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%level" />
      </layout>
    </parameter>
    <parameter>
      <parameterName value="@server" />
      <dbType value="string" />
      <size value="255" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%property{log4net:HostName}" />
      </layout>
    </parameter>
    <parameter>
      <parameterName value="@logger" />
      <dbType value="String" />
      <size value="255" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%logger" />
      </layout>
    </parameter>
    <parameter>
      <parameterName value="@method" />
      <dbType value="String" />
      <size value="10" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%property{method}" />
      </layout>
    </parameter>
    <parameter>
      <parameterName value="@url" />
      <dbType value="String" />
      <size value="255" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%property{url}" />
      </layout>
    </parameter>
    <parameter>
      <parameterName value="@key" />
      <dbType value="String" />
      <size value="255" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%property{key}" />
      </layout>
    </parameter>
    <parameter>
      <parameterName value="@year" />
      <dbType value="String" />
      <size value="10" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%property{year}" />
      </layout>
    </parameter>
    <parameter>
      <parameterName value="@responseCode" />
      <dbType value="String" />
      <size value="10" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%property{responseCode}" />
      </layout>
    </parameter>
    <parameter>
      <parameterName value="@startTime" />
      <dbType value="DateTimeOffset" />
      <size value="7" />
      <layout type="log4net.Layout.RawPropertyLayout">
        <key value="startTime" />
      </layout>
    </parameter>
    <parameter>
      <parameterName value="@endTime" />
      <dbType value="DateTimeOffset" />
      <size value="7" />
      <layout type="log4net.Layout.RawPropertyLayout">
        <key value="endTime" />
      </layout>
    </parameter>
    <parameter>
      <parameterName value="@dataExchangeMessage" />
      <dbType value="String" />
      <size value="500" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%property{dataExchangeMessage}" />
      </layout>
    </parameter>
    <parameter>
      <parameterName value="@correlationId" />
      <dbType value="String" />
      <size value="36" />
      <layout type="log4net.Layout.RawPropertyLayout">
        <key value="correlationId" />
      </layout>
    </parameter>
  </appender>
  <appender name="AdoNetExceptionAppender" type="Michigan.Ods.log4netAppender.AsyncAdoNetAppender, Michigan.Ods" >
    <bufferSize value="1" />
    <connectionType value="Microsoft.Data.SqlClient.SqlConnection, Microsoft.Data.SqlClient, Version=1.0.0.0,Culture=neutral,PublicKeyToken=23ec7fc2d6eaa4a5"/>
    <connectionStringName value="EdFi_Logging" />
    <connectionStringFile value="appsettings.json" />
    <ReconnectOnError value="true" />
    <commandText value="insert into &quot;api&quot;.&quot;exception&quot; (&quot;date&quot;,&quot;thread&quot;,&quot;level&quot;,&quot;logger&quot;,&quot;server&quot;,&quot;method&quot;,&quot;url&quot;,&quot;key&quot;,&quot;year&quot;,&quot;message&quot;,&quot;exception&quot;,&quot;correlationId&quot;) values (@log_date,@thread,@log_level,@logger,@server,@method,@url,@key,@year,@message,@exception,@correlationId);" />
    <parameter>
      <parameterName value="@log_date" />
      <dbType value="DateTime" />
      <layout type="log4net.Layout.RawTimeStampLayout" />
    </parameter>
    <parameter>
      <parameterName value="@thread" />
      <dbType value="String" />
      <size value="255" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%thread" />
      </layout>
    </parameter>
    <parameter>
      <parameterName value="@log_level" />
      <dbType value="String" />
      <size value="50" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%level" />
      </layout>
    </parameter>
    <parameter>
      <parameterName value="@server" />
      <dbType value="string" />
      <size value="255" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%property{log4net:HostName}" />
      </layout>
    </parameter>
    <parameter>
      <parameterName value="@logger" />
      <dbType value="String" />
      <size value="255" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%logger" />
      </layout>
    </parameter>
    <parameter>
      <parameterName value="@method" />
      <dbType value="String" />
      <size value="10" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%property{method}" />
      </layout>
    </parameter>
    <parameter>
      <parameterName value="@url" />
      <dbType value="String" />
      <size value="255" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%property{url}" />
      </layout>
    </parameter>
    <parameter>
      <parameterName value="@key" />
      <dbType value="String" />
      <size value="255" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%property{key}" />
      </layout>
    </parameter>
    <parameter>
      <parameterName value="@year" />
      <dbType value="String" />
      <size value="10" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%property{year}" />
      </layout>
    </parameter>
    <parameter>
      <parameterName value="@message" />
      <dbType value="String" />
      <size value="4000" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%message" />
      </layout>
    </parameter>
    <parameter>
      <parameterName value="@exception" />
      <dbType value="String" />
      <size value="-1" />
      <layout type="log4net.Layout.ExceptionLayout" />
    </parameter>
    <parameter>
      <parameterName value="@correlationId" />
      <dbType value="String" />
      <size value="36" />
      <layout type="log4net.Layout.RawPropertyLayout">
        <key value="correlationId" />
      </layout>
    </parameter>
  </appender>
  <root>
    <level value="$LOGLEVEL_ROOT" />
    <appender-ref ref="ConsoleAppender" />
    <appender-ref ref="AdoNetExceptionAppender" />
  </root>
  <logger name="CepiRestClient">
    <level value="$LOGLEVEL_CEPI" />
  </logger>
  <logger name="NHibernate">
    <level value="WARN"/>
  </logger>
  <logger name="Usage" additivity="false">
    <level value ="INFO"/>
    <appender-ref ref="AdoNetUsageAppender" />
  </logger>
</log4net>
